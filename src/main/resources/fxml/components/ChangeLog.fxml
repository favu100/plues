<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1" prefWidth="800" prefHeight="600"
         styleClass="windowPadding">
    <Label text="%temporary" styleClass="h2"/>
    <TableView fx:id="tempTable">
        <columns>
            <TableColumn fx:id="tableColumnSessionTemporary" text="%session" sortable="false"
                         resizable="false"/>
            <TableColumn fx:id="tableColumnSourceDayTemporary" text="%sourceDay" sortable="false"
                         resizable="false"/>
            <TableColumn fx:id="tableColumnSourceTimeTemporary" text="%sourceTime"
                         sortable="false" resizable="false"/>
            <TableColumn fx:id="tableColumnTargetDayTemporary" text="%targetDay" sortable="false"
                         resizable="false"/>
            <TableColumn fx:id="tableColumnTargetTimeTemporary" text="%targetTime" sortable="false"
                         resizable="false"/>
            <TableColumn fx:id="tableColumnDateTemporary" text="%date" sortable="false"
                         resizable="false"/>
        </columns>
    </TableView>
    <Label/>
    <Label text="%persistent" styleClass="h2"/>
    <TableView fx:id="persistentTable">
        <columns>
            <TableColumn fx:id="tableColumnSessionPersistent" text="%session" sortable="false"
                         resizable="false"/>
            <TableColumn fx:id="tableColumnSourceDayPersistent" text="%sourceDay" sortable="false"
                         resizable="false"/>
            <TableColumn fx:id="tableColumnSourceTimePersistent" text="%sourceTime"
                         sortable="false" resizable="false"/>
            <TableColumn fx:id="tableColumnTargetDayPersistent" text="%targetDay" sortable="false"
                         resizable="false"/>
            <TableColumn fx:id="tableColumnTargetTimePersistent" text="%targetTime" sortable="false"
                         resizable="false"/>
            <TableColumn fx:id="tableColumnDatePersistent" text="%date" sortable="false"
                         resizable="false"/>
        </columns>
    </TableView>
</fx:root>
