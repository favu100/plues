<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1" prefWidth="800" prefHeight="600" styleClass="windowPadding">
    <Label text="%temporary" styleClass="h2"/>
    <TableView fx:id="tempTable">
        <columns>
            <TableColumn fx:id="sessionT" text="%session" sortable="false"/>
            <TableColumn fx:id="sourceT" text="%source" sortable="false" minWidth="150" maxWidth="150"/>
            <TableColumn fx:id="targetT" text="%target" sortable="false" minWidth="150" maxWidth="150"/>
            <TableColumn fx:id="dateT" text="%date" sortable="false" minWidth="250" maxWidth="250"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
    <Label/>
    <Label text="%persistent" styleClass="h2"/>
    <TableView fx:id="persistentTable">
        <columns>
            <TableColumn fx:id="sessionP" text="%session" sortable="false"/>
            <TableColumn fx:id="sourceP" text="%source" sortable="false" minWidth="150" maxWidth="150"/>
            <TableColumn fx:id="targetP" text="%target" sortable="false" minWidth="150" maxWidth="150"/>
            <TableColumn fx:id="dateP" text="%date" sortable="false" minWidth="250" maxWidth="250"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</fx:root>
