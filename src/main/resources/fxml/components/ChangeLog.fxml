<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.text.Font?>
<fx:root type="VBox"
         xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="%temporary">
            <font>
                <Font size="20"/>
            </font></Label>
        <TableView fx:id="tempTable">
            <columns>
                <TableColumn fx:id="sessionT" text="%session" sortable="false"
                             prefWidth="400" resizable="false"/>
                <TableColumn fx:id="sourceT" text="%source" sortable="false"
                             prefWidth="150" resizable="false"/>
                <TableColumn fx:id="targetT" text="%target" sortable="false"
                             prefWidth="100" resizable="false"/>
                <TableColumn fx:id="dateT" text="%date" sortable="false"
                             prefWidth="250" resizable="false"/>
            </columns>
        </TableView>
        <Label text="%persistent">
            <font>
                <Font size="20"/>
            </font>
        </Label>
        <TableView fx:id="persistentTable">
            <columns>
                <TableColumn fx:id="sessionP" text="%session" sortable="false"
                             prefWidth="400" resizable="false"/>
                <TableColumn fx:id="sourceP" text="%source" sortable="false"
                             prefWidth="150" resizable="false"/>
                <TableColumn fx:id="targetP" text="%target" sortable="false"
                             prefWidth="100" resizable="false"/>
                <TableColumn fx:id="dateP" text="%date" sortable="false"
                             prefWidth="250" resizable="false"/>
            </columns>
        </TableView>
    </children>
</fx:root>
