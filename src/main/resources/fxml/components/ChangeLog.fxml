<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<fx:root prefHeight="30.0" prefWidth="500.0" type="VBox"
         xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="%persistent" />
        <TableView fx:id="persistentTable">
        <columns>
            <TableColumn fx:id="session_p" text="%session"/>
            <TableColumn fx:id="source_p" text="%source"/>
            <TableColumn fx:id="target_p" text="%target"/>
            <TableColumn fx:id="date_p" text="%date"/>
        </columns>
        </TableView>
        <Label text="%temporary"/>
        <TableView fx:id="tempTable">
            <columns>
                <TableColumn fx:id="session_t" text="%session"/>
                <TableColumn fx:id="source_t" text="%source"/>
                <TableColumn fx:id="target_t" text="%target"/>
                <TableColumn fx:id="date_t" text="%date"/>
            </columns>
        </TableView>
    </children>
</fx:root>
