<?import de.hhu.stups.plues.ui.components.CombinationOrSingleCourseSelection?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ListView?>
<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1" spacing="10.0">
    <CombinationOrSingleCourseSelection fx:id="combinationOrSingleCourseSelection"/>
    <HBox>
        <Button fx:id="btCheckFeasibility" onAction="#checkFeasibility" text="%checkFeasibility"/>
        <Button fx:id="btUnhighlightAllConflicts" text="%unhighlightConflicts" visible="false"
                onAction="#unhighlightConflicts"/>
    </HBox>
    <ListView fx:id="resultBoxWrapper" id="batchListView" focusTraversable="false"
              VBox.vgrow="ALWAYS"/>
</fx:root>
