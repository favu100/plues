<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.control.TreeItem?>

<fx:root prefWidth="600.0" type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <TreeView fx:id="conflictTreeView">
        <TreeItem fx:id="conflictTreeRootItem" value="%conflictPaneTitle"/>
    </TreeView>
    <Button fx:id="btHighlightAllConflicts" text="%highlightConflicts"
            onAction="#highlightConflicts"/>
</fx:root>