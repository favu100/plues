<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Label?>
<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Session Info"/>
    <TableView fx:id="sessionTable">
        <columns>
            <TableColumn fx:id="keyColumn"/>
            <TableColumn fx:id="valueColumn"/>
        </columns>
    </TableView>
    <Label text="Related Courses and Modules"/>
    <TableView fx:id="courseTable">
        <columns>
            <TableColumn fx:id="courseName"/>
            <TableColumn fx:id="po"/>
            <TableColumn fx:id="moduleName"/>
        </columns>
    </TableView>
</fx:root>
