<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="%search"/>
        <TextField fx:id="txtQuery"/>
        <TitledPane fx:id="titledPaneMasterCourse" text="%master" maxHeight="300.0">
            <TableView fx:id="tableViewMasterCourse" prefWidth="500.0" maxHeight="300.0">
                <columns>
                    <TableColumn fx:id="tableColumnMasterCheckBox" prefWidth="30.0"/>
                    <TableColumn fx:id="tableColumnMasterCourse" prefWidth="470.0"
                                 minWidth="470.0" text="%course"/>
                </columns>
            </TableView>
        </TitledPane>
        <TitledPane fx:id="titledPaneBachelorCourse" text="%bachelor" prefWidth="400.0"
                    maxHeight="300.0">
            <TableView fx:id="tableViewBachelorCourse" prefWidth="500.0" maxHeight="300.0">
                <columns>
                    <TableColumn fx:id="tableColumnBachelorCheckBox" prefWidth="30.0"/>
                    <TableColumn fx:id="tableColumnBachelorCourse" prefWidth="470.0"
                                 minWidth="470.0" text="%course"/>
                </columns>
            </TableView>
        </TitledPane>
    </children>
</fx:root>
