<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.text.Text?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox styleClass="windowPaddingSmall" spacing="10.0">
            <Text fx:id="txtExplanation" text="%explain.ImpossibleAbstractUnitsInModule"/>
            <Text text="%modules" style="-fx-font-weight: bold;"/>
        </VBox>
        <ListView fx:id="listViewModules" focusTraversable="false"/>
        <VBox styleClass="windowPaddingSmall">
            <Label text="%abstractUnits" style="-fx-font-weight: bold;"/>
        </VBox>
        <TableView fx:id="tableViewAbstractUnits" focusTraversable="false">
            <columns>
                <TableColumn text="%columnKey" prefWidth="150.0">
                    <cellValueFactory>
                        <PropertyValueFactory property="key"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%columnTitle" prefWidth="550.0">
                    <cellValueFactory>
                        <PropertyValueFactory property="title"/>
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
    </children>
</fx:root>
