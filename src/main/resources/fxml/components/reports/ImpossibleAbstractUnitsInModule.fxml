<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>
<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="%explain.ImpossibleAbstractUnitsInModule"/>
        <Label text="%modules" style="-fx-font-size: 20px">
        </Label>
        <ListView fx:id="listViewModules"/>
        <Label text="%abstractUnits" style="-fx-font-size: 20px"/>
        <TableView fx:id="tableViewAbstractUnits">
            <columns>
                <TableColumn fx:id="columnAbstractUnitKey" text="%columnKey" prefWidth="150.0"/>
                <TableColumn fx:id="columnAbstractUnitTitle" text="%columnTitle"
                             prefWidth="550.0"/>
            </columns>
        </TableView>
    </children>
</fx:root>
