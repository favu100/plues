<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="%explain.ImpossibleAbstractUnitsInModule"/>
        <Label text="%modules" style="-fx-font-size: 20px">
        </Label>
        <ListView fx:id="listViewModules"/>
        <Label text="%abstractUnits" style="-fx-font-size: 20px"/>
        <TableView fx:id="tableViewAbstractUnits">
            <columns>
                <TableColumn text="%columnKey" prefWidth="150.0">
                    <cellValueFactory>
                        <PropertyValueFactory property="key"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="%columnTitle" prefWidth="550.0">
                    <cellValueFactory>
                        <PropertyValueFactory property="title"/>
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
    </children>
</fx:root>
