<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>
<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <Label text="%explainIncompleteModules"/>
    <Label text="%modules" style="-fx-font-size: 20px"/>
    <TableView fx:id="tableViewModules">
        <columns>
            <TableColumn fx:id="columnModulePordnr" prefWidth="100.0" text="%columnPordnr"/>
            <TableColumn fx:id="columnModuleTitle" prefWidth="600.0" text="%columnTitle"/>
        </columns>
    </TableView>
</fx:root>
