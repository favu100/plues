<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Label?>
<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="%explainAbstractUnitsWithUnits"/>
        <Label text="%modules" style="-fx-font-size: 20px">
        </Label>
        <ListView fx:id="listViewModules"/>
        <Label text="%abstractUnits" style="-fx-font-size: 20px"/>
        <TableView fx:id="tableViewAbstractUnitUnitSemesters">
            <columns>
                <TableColumn fx:id="tableColumnAbstractUnit" text="%columnAbstractUnit"
                             minWidth="100.0" prefWidth="350.0"/>
                <TableColumn fx:id="tableColumnUnit" text="%columnExplicitUnit"
                             minWidth="100.0" prefWidth="350.0"/>
                <TableColumn fx:id="tableColumnSemesters" text="%columnSemesters"
                             minWidth="80.0" prefWidth="100.0"/>
            </columns>
        </TableView>
    </children>
</fx:root>
