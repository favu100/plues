<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <Label text="%explain.AbstractUnitsWithUnits"/>
    <Label text="%modules" style="-fx-font-size: 20px">
    </Label>
    <TableView fx:id="tableViewModules">
        <columns>
            <TableColumn minWidth="100.0" prefWidth="100.0" text="%columnPordnr">
                <cellValueFactory>
                    <PropertyValueFactory property="pordnr"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn minWidth="300.0" prefWidth="400.0" text="%columnTitle">
                <cellValueFactory>
                    <PropertyValueFactory property="title"/>
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
    <Label text="%abstractUnits" style="-fx-font-size: 20px"/>
    <TableView fx:id="tableViewAbstractUnitUnitSemesters">
        <columns>
            <TableColumn text="%columnAbstractUnit">
                <columns>
                    <TableColumn text="%columnKey" minWidth="50.0" prefWidth="100.0">
                        <cellValueFactory>
                            <PropertyValueFactory property="abstractUnitKey"/>
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn text="%columnTitle" minWidth="100.0" prefWidth="300.0">
                        <cellValueFactory>
                            <PropertyValueFactory property="abstractUnitTitle"/>
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn text="%columnSemesters" minWidth="50.0" prefWidth="50.0">
                        <cellValueFactory>
                            <PropertyValueFactory property="abstractUnitSemesters"/>
                        </cellValueFactory>
                    </TableColumn>
                </columns>
            </TableColumn>
            <TableColumn text="%columnExplicitUnit">
                <columns>
                    <TableColumn text="%columnKey" minWidth="50.0" prefWidth="100.0">
                        <cellValueFactory>
                            <PropertyValueFactory property="unitKey"/>
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn text="%columnTitle" minWidth="100.0" prefWidth="300.0">
                        <cellValueFactory>
                            <PropertyValueFactory property="unitTitle"></PropertyValueFactory>
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn text="%columnSemesters" minWidth="50.0" prefWidth="50.0">
                        <cellValueFactory>
                            <PropertyValueFactory property="unitSemesters"/>
                        </cellValueFactory>
                    </TableColumn>
                </columns>
            </TableColumn>
        </columns>
    </TableView>
</fx:root>
