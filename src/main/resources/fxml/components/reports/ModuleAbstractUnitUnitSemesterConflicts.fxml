<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <Label text="%explainAbstractUnitsWithUnits"/>
    <Label text="%modules" style="-fx-font-size: 20px">
    </Label>
    <ListView fx:id="listViewModules"/>
    <Label text="%abstractUnits" style="-fx-font-size: 20px"/>
    <TableView fx:id="tableViewAbstractUnitUnitSemesters">
        <columns>
            <TableColumn text="%columnAbstractUnit">
                <columns>
                    <TableColumn fx:id="tableColumnAbstractUnitKey" text="%columnAbstractUnitKey"
                                 minWidth="50.0" prefWidth="100.0"/>
                    <TableColumn fx:id="tableColumnAbstractUnitTitle" text="%columnTitle"
                                 minWidth="100.0" prefWidth="300.0"/>
                    <TableColumn fx:id="tableColumnAbstractUnitSemesters" text="%columnSemesters"
                                 minWidth="50.0" prefWidth="50.0"/>
                </columns>
            </TableColumn>
            <TableColumn text="%columnExplicitUnit">
                <columns>
                    <TableColumn fx:id="tableColumnUnitKey" text="%columnUnitKey"
                                 minWidth="50.0" prefWidth="100.0"/>
                    <TableColumn fx:id="tableColumnUnitTitle" text="%columnUnitTitle"
                                 minWidth="100.0" prefWidth="300.0"/>
                    <TableColumn fx:id="tableColumnUnitSemesters" text="%columnSemesters"
                                 minWidth="50.0" prefWidth="50.0"/>
                </columns>
            </TableColumn>
        </columns>
    </TableView>
</fx:root>
