<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.76-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <Label text="%explain.QuasiMandatoryModules" wrapText="true"/>
    <VBox spacing="5.0">
        <Label text="%modules" style="-fx-font-size: 20px"/>
        <TableView fx:id="tableViewQuasiMandatoryModules">
            <columns>
                <TableColumn fx:id="columnModulePordnr" text="%columnPordnr"
                             prefWidth="200.0" minWidth="100.0"/>
                <TableColumn fx:id="columnModuleTitle" text="%columnTitle"
                             prefWidth="500.0" minWidth="100.0"/>
            </columns>
        </TableView>
        <Label text="%abstractUnits" style="-fx-font-size: 20px"/>
        <TableView fx:id="tableViewAbstractUnits">
            <columns>
                <TableColumn fx:id="columnAbstractUnitKey" text="%columnKey"
                             prefWidth="200.0" minWidth="100.0"/>
                <TableColumn fx:id="columnAbstractUnitTitle" text="%columnTitle"
                             prefWidth="500.0" minWidth="100.0"/>
            </columns>
        </TableView>
    </VBox>
</fx:root>
