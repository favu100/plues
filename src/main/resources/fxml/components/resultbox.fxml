<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root prefHeight="100.0" prefWidth="200.0" type="GridPane" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane fx:id="icon" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.rowSpan="3"/>
      <Label fx:id="major" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.columnSpan="3"/>
      <Label fx:id="minor" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.columnSpan="3"/>
      <Button fx:id="show" disable="true" mnemonicParsing="false" text="Show" textAlignment="CENTER"
         GridPane.columnIndex="1" GridPane.rowIndex="3"/>
      <Button fx:id="download" disable="true" mnemonicParsing="false" text="Download" textAlignment="CENTER"
         GridPane.columnIndex="2" GridPane.rowIndex="3"/>
      <Button fx:id="cancel" disable="true" mnemonicParsing="false" text="Cancel" textAlignment="CENTER"
         GridPane.columnIndex="3" GridPane.rowIndex="3" onAction="#interrupt"/>
   </children>
</fx:root>
