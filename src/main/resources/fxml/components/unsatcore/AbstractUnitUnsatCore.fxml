<?xml version="1.0" encoding="UTF-8"?>

<?import de.hhu.stups.plues.ui.controller.unsatcore.UnsatCoreButtonBar?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<fx:root type="VBox" spacing="10.0" styleClass="windowPadding" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1">
    <Text fx:id="txtExplanation" text="%abstractUnitsPaneExplanation"/>
    <TableView fx:id="abstractUnitsTable">
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
        <columns>
            <TableColumn maxWidth="-1.0" minWidth="200.0" prefWidth="-1.0" text="%columnKey">
                <cellValueFactory>
                    <PropertyValueFactory property="key"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn maxWidth="1.7976931348623157E308" minWidth="200" prefWidth="-1.0"
                         text="%columnTitle">
                <cellValueFactory>
                    <PropertyValueFactory property="title"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="abstractUnitModuleSemester" maxWidth="-1.0" minWidth="200.0"
                         prefWidth="-1.0" text="%columnSemesters"/>
            <TableColumn fx:id="abstractUnitModuleType" maxWidth="-1.0" minWidth="200.0"
                         prefWidth="-1.0" text="%columnType"/>
        </columns>
    </TableView>
    <UnsatCoreButtonBar fx:id="unsatCoreButtonBar"/>
</fx:root>
