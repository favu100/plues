<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import de.hhu.stups.plues.ui.components.TaskProgressIndicator?>

<fx:root prefWidth="700.0" type="HBox" spacing="10.0" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1">
    <Button fx:id="btSubmitTask"/>
    <Button fx:id="btCancelTask" text="%button.cancel" onAction="#cancelTask" disable="true"/>
    <TaskProgressIndicator fx:id="taskProgressIndicator"/>
</fx:root>
