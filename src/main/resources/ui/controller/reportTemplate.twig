<?xml version="1.0" encoding="utf-8"?>
<fo:root xmlns:fo="http://www.w3.org/1999/XSL/Format" font-family="sans-serif">
    <fo:layout-master-set>
        <fo:simple-page-master master-name="hoch" page-width="21.0cm" page-height="29.7cm" margin="1cm">
            <fo:region-body extent="1cm"/>
        </fo:simple-page-master>
        <fo:simple-page-master master-name="quer" page-width="29.7cm" page-height="21.0cm" margin="1cm">
            <fo:region-body extent="1cm"/>
        </fo:simple-page-master>
        <fo:simple-page-master master-name="first" page-width="21.0cm" page-height="29.7cm" margin="1cm">
            <fo:region-body extent="1cm"/>
        </fo:simple-page-master>
    </fo:layout-master-set>
    <fo:page-sequence master-reference="first">
        <fo:flow flow-name="xsl-region-body">
            <fo:block text-align="right">
                <fo:inline>
                    <fo:external-graphic src="{{ logo }}"
                                     content-height="5cm" content-width="7cm"/>
                </fo:inline>
            </fo:block>
            <fo:block text-align="center" font-size="50pt" font-weight="bold">
                Bericht von Pl√úS
            </fo:block>
            <fo:block text-align="center" font-size="30pt" font-style="italic">
                {{ faculty }}
            </fo:block>
            <fo:block text-align="center" font-size="30pt" font-style="italic">
                {{ date }}
            </fo:block>
        </fo:flow>
    </fo:page-sequence>
    <fo:page-sequence master-reference="hoch">
        <fo:flow flow-name="xsl-region-body">
            <fo:block font-size="18pt">
                1. {{ resources.get("title.ImpossibleModules") }}
            </fo:block>
            {% if incompleteModules|length > 0 %}
                <fo:block font-size="16pt">
                    1.1 {{ resources.get("title.IncompleteModules") }}
                </fo:block>
                <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                    <fo:table-column column-width="30%"/>
                    <fo:table-column column-width="70%"/>
                    <fo:table-header>
                        <fo:table-row height="0.4cm" font-size="12pt">
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnPordnr") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnTitle") }}</fo:block>
                            </fo:table-cell>
                        </fo:table-row>
                    </fo:table-header>
                    <fo:table-body>
                        {% for module in incompleteModules %}
                            <fo:table-row height="0.4cm">
                                <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-right="2pt">
                                    <fo:block text-align="right">{{ module.getPordnr() }}</fo:block>
                                </fo:table-cell>
                                <fo:table-cell border-style="solid" border-width="0.5pt" font-size="8pt" padding-left="2pt">
                                    <fo:block text-align="left">{{ module.getTitle() }}</fo:block>
                                </fo:table-cell>
                            </fo:table-row>
                        {% endfor %}
                    </fo:table-body>
                </fo:table>
            {% endif %}
            {% if impossibleModulesBecauseOfMissingElectiveAbstractUnits|length > 0 %}
                <fo:block font-size="16pt">
                    1.2 {{ resources.get("title.ImpossibleModulesBecauseOfMissingElectiveAbstractUnits") }}
                </fo:block>
                <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                   <fo:table-column column-width="30%"/>
                    <fo:table-column column-width="70%"/>
                    <fo:table-header>
                        <fo:table-row height="0.4cm" font-size="12pt">
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnPordnr") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnTitle") }}</fo:block>
                            </fo:table-cell>
                        </fo:table-row>
                    </fo:table-header>
                    <fo:table-body>
                        {% for module in impossibleModulesBecauseOfMissingElectiveAbstractUnits %}
                            <fo:table-row height="0.4cm">
                                <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-right="2pt">
                                    <fo:block text-align="right">{{ module.getPordnr() }}</fo:block>
                                </fo:table-cell>
                                <fo:table-cell border-style="solid" border-width="0.5pt" font-size="8pt" padding-left="2pt">
                                    <fo:block text-align="left">{{ module.getTitle() }}</fo:block>
                                </fo:table-cell>
                            </fo:table-row>
                        {% endfor %}
                    </fo:table-body>
                </fo:table>
            {% endif %}
        </fo:flow>
    </fo:page-sequence>
    <fo:page-sequence master-reference="hoch">
        <fo:flow flow-name="xsl-region-body">
            <fo:block font-size="18pt">
                2. {{ resources.get("title.ImpossibleCourses") }}
            </fo:block>
            {% if impossibleCourses|length > 0 %}
                <fo:block font-size="16pt">
                    2.1 {{ resources.get("title.ImpossibleCourses") }}
                </fo:block>
               <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                   <fo:table-column column-width="30%"/>
                   <fo:table-column column-width="70%"/>
                   <fo:table-header>
                       <fo:table-row height="0.4cm" font-size="12pt">
                           <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                               <fo:block text-align="center">{{ resources.get("columnKey") }}</fo:block>
                           </fo:table-cell>
                           <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                               <fo:block text-align="center">{{ resources.get("columnName") }}</fo:block>
                           </fo:table-cell>
                       </fo:table-row>
                   </fo:table-header>
                   <fo:table-body>
                       {% for course in impossibleCourses %}
                           <fo:table-row height="0.4cm">
                               <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                   <fo:block text-align="left">{{ course.getKey() }}</fo:block>
                               </fo:table-cell>
                               <fo:table-cell border-style="solid" border-width="0.5pt" font-size="8pt" padding-left="2pt">
                                   <fo:block text-align="left">{{ course.getFullName() }}</fo:block>
                               </fo:table-cell>
                           </fo:table-row>
                       {% endfor %}
                   </fo:table-body>
               </fo:table>
           {% endif %}
           {% if impossibleCoursesBecauseOfImpossibleModules|length > 0 %}
                <fo:block font-size="16pt">
                    2.2 {{ resources.get("title.ImpossibleCoursesBecauseOfImpossibleModules") }}
                </fo:block>
               <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                   <fo:table-column column-width="30%"/>
                   <fo:table-column column-width="70%"/>
                   <fo:table-header>
                       <fo:table-row height="0.4cm" font-size="12pt">
                           <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                               <fo:block text-align="center">{{ resources.get("columnKey") }}</fo:block>
                           </fo:table-cell>
                           <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                               <fo:block text-align="center">{{ resources.get("columnName") }}</fo:block>
                           </fo:table-cell>
                       </fo:table-row>
                   </fo:table-header>
                   <fo:table-body>
                       {% for course in impossibleCoursesBecauseOfImpossibleModules %}
                           <fo:table-row height="0.4cm">
                               <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                   <fo:block text-align="left">{{ course.getKey() }}</fo:block>
                               </fo:table-cell>
                               <fo:table-cell border-style="solid" border-width="0.5pt" font-size="8pt" padding-left="2pt">
                                   <fo:block text-align="left">{{ course.getFullName() }}</fo:block>
                               </fo:table-cell>
                           </fo:table-row>
                       {% endfor %}
                   </fo:table-body>
               </fo:table>
           {% endif %}
           {% if impossibleCoursesBecauseOfImpossibleModuleCombinations|length > 0 %}
                <fo:block font-size="16pt">
                    2.3 {{ resources.get("title.ImpossibleCoursesBecauseOfImpossibleModuleCombinations") }}
                </fo:block>
                <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                    <fo:table-column column-width="30%"/>
                    <fo:table-column column-width="70%"/>
                    <fo:table-header>
                        <fo:table-row height="0.4cm" font-size="12pt">
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnKey") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnName") }}</fo:block>
                            </fo:table-cell>
                        </fo:table-row>
                    </fo:table-header>
                    <fo:table-body>
                        {% for course in impossibleCoursesBecauseOfImpossibleModuleCombinations %}
                            <fo:table-row height="0.4cm">
                                <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                    <fo:block text-align="left">{{ course.getKey() }}</fo:block>
                                </fo:table-cell>
                                <fo:table-cell border-style="solid" border-width="0.5pt" font-size="8pt" padding-left="2pt">
                                    <fo:block text-align="left">{{ course.getFullName() }}</fo:block>
                                </fo:table-cell>
                            </fo:table-row>
                        {% endfor %}
                    </fo:table-body>
                </fo:table>
            {% endif %}
        </fo:flow>
    </fo:page-sequence>
    <fo:page-sequence master-reference="hoch">
        <fo:flow flow-name="xsl-region-body">
            <fo:block font-size="18pt">
                3. {{ resources.get("title.AbstractUnits") }}
            </fo:block>
            {% if abstractUnitsWithoutUnits|length > 0 %}
               <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                   <fo:table-column column-width="30%"/>
                   <fo:table-column column-width="70%"/>
                   <fo:table-header>
                       <fo:table-row height="0.4cm" font-size="12pt">
                           <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                               <fo:block text-align="center">{{ resources.get("columnKey") }}</fo:block>
                           </fo:table-cell>
                           <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                               <fo:block text-align="center">{{ resources.get("columnTitle") }}</fo:block>
                           </fo:table-cell>
                       </fo:table-row>
                   </fo:table-header>
                   <fo:table-body>
                       {% for abstractUnit in abstractUnitsWithoutUnits %}
                           <fo:table-row height="0.4cm">
                               <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                   <fo:block text-align="left">{{ abstractUnit.getKey() }}</fo:block>
                               </fo:table-cell>
                               <fo:table-cell border-style="solid" border-width="0.5pt" font-size="8pt" padding-left="2pt">
                                   <fo:block text-align="left">{{ abstractUnit.getTitle() }}</fo:block>
                               </fo:table-cell>
                           </fo:table-row>
                       {% endfor %}
                   </fo:table-body>
               </fo:table>
           {% endif %}
        </fo:flow>
    </fo:page-sequence>
    <fo:page-sequence master-reference="hoch">
        <fo:flow flow-name="xsl-region-body">
            <fo:block font-size="18pt">
                4. {{ resources.get("title.UnitsWithoutAbstractUnits") }}
            </fo:block>
            {% if unitsWithoutAbstractUnits|length > 0 %}
                <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                    <fo:table-column column-width="20%"/>
                    <fo:table-column column-width="80%"/>
                    <fo:table-header>
                        <fo:table-row height="0.4cm" font-size="12pt">
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnKey") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnTitle") }}</fo:block>
                            </fo:table-cell>
                        </fo:table-row>
                    </fo:table-header>
                    <fo:table-body>
                        {% for unit in unitsWithoutAbstractUnits %}
                            <fo:table-row height="0.4cm">
                                <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-right="2pt">
                                    <fo:block text-align="right">{{ unit.getKey() }}</fo:block>
                                </fo:table-cell>
                                <fo:table-cell border-style="solid" border-width="0.5pt" font-size="8pt" padding-left="2pt">
                                    <fo:block text-align="left">{{ unit.getTitle() }}</fo:block>
                                </fo:table-cell>
                            </fo:table-row>
                        {% endfor %}
                    </fo:table-body>
                </fo:table>
            {% endif %}
        </fo:flow>
    </fo:page-sequence>
    <fo:page-sequence master-reference="quer">
        <fo:flow flow-name="xsl-region-body">
            <fo:block font-size="18pt">
                5. {{ resources.get("title.AbstractUnitsWithUnits") }}
            </fo:block>
            {% if moduleAbstractUnitUnitSemesterConflicts|length > 0 %}
                <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                    <fo:table-column column-width="7.0%"/>
                    <fo:table-column column-width="16.3%"/>
                    <fo:table-column column-width="8.0%"/>
                    <fo:table-column column-width="17.3%"/>
                    <fo:table-column column-width="8.0%"/>
                    <fo:table-column column-width="8.0%"/>
                    <fo:table-column column-width="19.3%"/>
                    <fo:table-column column-width="8.0%"/>
                    <fo:table-header>
                        <fo:table-row height="0.4cm" font-size="12pt">
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt" number-columns-spanned="2">
                                <fo:block text-align="center">{{ resources.get("columnModule") }}</fo:block>
                            </fo:table-cell>
                           <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt" number-columns-spanned="3">
                                <fo:block text-align="center">{{ resources.get("columnAbstractUnit") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt" number-columns-spanned="3">
                                <fo:block text-align="center">{{ resources.get("columnExplicitUnit") }}</fo:block>
                            </fo:table-cell>
                        </fo:table-row>
                        <fo:table-row height="0.4cm" font-size="10pt">
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnPordnr") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnTitle") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnKey") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnTitle") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnSemesters") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnKey") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnTitle") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnSemesters") }}</fo:block>
                            </fo:table-cell>
                        </fo:table-row>
                    </fo:table-header>
                    <fo:table-body>
                        {% for module in moduleAbstractUnitUnitSemesterConflicts.entrySet() %}
                            {% for conflict in module.value %}
                                <fo:table-row height="0.4cm">
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-right="2pt">
                                        <fo:block text-align="right">
                                            {{ module.key.getPordnr() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                        <fo:block text-align="left">
                                            {{ module.key.getTitle() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                        <fo:block text-align="left">
                                            {{ conflict.getAbstractUnitKey() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                        <fo:block text-align="left">
                                            {{ conflict.getAbstractUnitTitle() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-right="2pt">
                                        <fo:block text-align="right">
                                            {{ conflict.getAbstractUnitSemesters() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-right="2pt">
                                        <fo:block text-align="right">
                                            {{ conflict.getUnitKey() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                        <fo:block text-align="left">
                                            {{ conflict.getUnitTitle() }},
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-right="2pt">
                                        <fo:block text-align="right">
                                            {{ conflict.getUnitSemesters() }}
                                        </fo:block>
                                    </fo:table-cell>
                                </fo:table-row>
                            {% endfor %}
                        {% endfor %}
                    </fo:table-body>
                </fo:table>
            {% endif %}
        </fo:flow>
    </fo:page-sequence>
    <fo:page-sequence master-reference="quer">
        <fo:flow flow-name="xsl-region-body">
            <fo:block font-size="18pt">
                6. {{ resources.get("title.MandatoryModules") }}
            </fo:block>
            {% if mandatoryModules|length > 0 %}
                <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                    <fo:table-column column-width="10.0%"/>
                    <fo:table-column column-width="40.0%"/>
                    <fo:table-column column-width="10.0%"/>
                    <fo:table-column column-width="30.0%"/>
                    <fo:table-column column-width="10.0%"/>
                    <fo:table-header>
                        <fo:table-row height="0.4cm" font-size="12pt">
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt" number-columns-spanned="2">
                                <fo:block text-align="center">{{ resources.get("columnCourse") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt" number-columns-spanned="3">
                                <fo:block text-align="center">{{ resources.get("columnModule") }}</fo:block>
                            </fo:table-cell>
                        </fo:table-row>
                        <fo:table-row height="0.4cm" font-size="10pt">
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnKey") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnTitle") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnKey") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnTitle") }}</fo:block>
                            </fo:table-cell>
                            <fo:table-cell font-weight="bold" border-style="solid" border-width="2pt">
                                <fo:block text-align="center">{{ resources.get("columnElectability") }}</fo:block>
                            </fo:table-cell>
                        </fo:table-row>
                    </fo:table-header>
                    <fo:table-body>
                        {% for course in mandatoryModules.entrySet() %}
                            {% for module in course.value %}
                                <fo:table-row height="0.4cm">
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                        <fo:block text-align="left">
                                            {{ course.key.getKey() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                        <fo:block text-align="left">
                                            {{ course.key.getFullName() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-right="2pt">
                                        <fo:block text-align="right">
                                            {{ module.getPordnr() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                        <fo:block text-align="left">
                                            {{ module.getTitle() }}
                                        </fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell border-style="solid" border-width="0.5pt" font-size="10pt" padding-left="2pt">
                                        <fo:block text-align="center">
                                            {{ module.getMandatory() }}
                                        </fo:block>
                                    </fo:table-cell>
                                </fo:table-row>
                            {% endfor %}
                        {% endfor %}
                    </fo:table-body>
                </fo:table>
            {% endif %}
        </fo:flow>
    </fo:page-sequence>
    <fo:page-sequence master-reference="quer">
        <fo:flow flow-name="xsl-region-body">
            <fo:block font-size="18pt">
                7. {{ resources.get("title.QuasiMandatoryModules") }}
            </fo:block>
            {% if quasiMandatoryModuleAbstractUnits|length > 0 %}
               <fo:table border-style="transparent" width="100%" table-layout="fixed" break-after="page">
                   <fo:table-column column-width="10.0%"/>
                   <fo:table-column column-width="40.0%"/>
                   <fo:table-column column-width="10.0%"/>
                   <fo:table-column column-width="40.0%"/>
                   <fo:table-header>
                       <fo:table-row height="0.4cm" font-size="12pt">
